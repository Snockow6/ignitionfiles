variant: fcos
version: 1.4.0
storage:
  disks:
  -
    device: /dev/by-id/coreos-boot-disk
    wipe_table: false
    partitions:
    - number: 4
      label: root
      size_mib: 8192
      resize: true
    - size_mib: 0
      label: var
    filesystems:
      - path: /var
        device: /dev/disk/by-partlabel/var
        format: btrfs
        with_mount_unit: true
  users:
    - name: nathan
      groups:
        - wheel
      password_hash: $y$j9T$UJBWzpzCD9bEX3WjQlGkU1$5LLzqOxhpKuz4qMWadyZvZqrmwgBe7yy2/BX5GUD9l9
      ssh_authorized_keys:
        - ssh-rsa  nathan@nathan-lp

systemd:
  units:
    - name: podman.socket
      enabled: true
    
    - name: cockpit.socket
      enabled: true
